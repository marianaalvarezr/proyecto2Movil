<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AmpliandoRestaurante">
    <TextView
        android:id="@+id/titulorestampliando"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:text="Estrella del mar"
        android:textColor="@color/color2"
        android:textStyle="bold"
        android:textSize="25dp"
        app:layout_constraintVertical_bias="0.05"
        />
    <ImageView
        android:id="@+id/fotorestampliando"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintTop_toBottomOf="@+id/titulorestampliando"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:src="@drawable/estrelladelmar"
        app:layout_constraintVertical_bias="0"
        app:layout_constraintWidth="420dp"
        app:layout_constraintHeight="190dp"
        />

    <TextView
        android:id="@+id/numerorestampliando"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="313513897"
        android:textSize="18dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.273"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/contacotitulo"
        app:layout_constraintVertical_bias="0.004" />

    <TextView
        android:id="@+id/preciorestampliando"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="$20.000 - $40.000"
        android:textSize="18dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.788"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/preciotitulo"
        app:layout_constraintVertical_bias="0.002" />

    <TextView
        android:id="@+id/preciotitulo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/precio"
        android:textColor="@color/color2"
        android:textSize="18dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.7"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fotorestampliando"
        app:layout_constraintVertical_bias="0.037" />

    <TextView
        android:id="@+id/contacotitulo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/contacto"
        android:textColor="@color/color2"
        android:textSize="18dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.284"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fotorestampliando"
        app:layout_constraintVertical_bias="0.035" />

    <TextView
        android:id="@+id/platorecomendadotitulo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/platorecomendado"
        android:textColor="@color/color2"
        android:textSize="18dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/numerorestampliando"
        app:layout_constraintVertical_bias="0.026" />

    <TextView
        android:id="@+id/platorecomendado"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Robalo"
        android:textSize="18dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/numerorestampliando"
        app:layout_constraintVertical_bias="0.088" />

    <TextView
        android:id="@+id/descripcionrestampliando"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="30dp"
        android:layout_marginRight="30dp"
        android:layout_marginBottom="252dp"
        android:text="@string/descripcionrestaurante"
        android:textAlignment="textStart"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/platorecomendado"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/imagenrest1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/rest1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight="60dp"
        app:layout_constraintHorizontal_bias="0.195"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/descripcionrestampliando"
        app:layout_constraintVertical_bias="0.103"
        app:layout_constraintWidth="140dp" />

    <ImageView
        android:id="@+id/imagenrest2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/rest2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight="60dp"
        app:layout_constraintHorizontal_bias="0.784"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/descripcionrestampliando"
        app:layout_constraintVertical_bias="0.103"
        app:layout_constraintWidth="110dp" />

    <TextView
        android:id="@+id/comentarioampliandorest"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="100dp"
        android:layout_marginRight="40dp"
        android:text="Es un hrestaurante con comida muy rica"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.226"
        app:layout_constraintStart_toEndOf="@+id/iconoperfil"
        app:layout_constraintTop_toBottomOf="@+id/imagenrest1"
        app:layout_constraintVertical_bias="0.161" />

    <ImageView
        android:id="@+id/iconoperfil"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/perfilamarillo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight="60dp"
        app:layout_constraintHorizontal_bias="0.076"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imagenrest1"
        app:layout_constraintVertical_bias="0.112"
        app:layout_constraintWidth="60dp" />

    <ImageView
        android:id="@+id/comillas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/comillasamarillas"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight="20dp"
        app:layout_constraintHorizontal_bias="0.892"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imagenrest1"
        app:layout_constraintVertical_bias="0.151"
        app:layout_constraintWidth="20dp" />


    <TextView
        android:id="@+id/calificaciontitulo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/calificacion"
        android:textColor="@color/color2"
        android:textSize="18dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.236"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iconoperfil"
        app:layout_constraintVertical_bias="0.124" />

    <ImageView
        android:id="@+id/estrellas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/estrellas"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight="50dp"
        app:layout_constraintHorizontal_bias="0.578"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/comillas"
        app:layout_constraintVertical_bias="0.239"
        app:layout_constraintWidth="50dp" />

    <TextView
        android:id="@+id/calificaionampliandorest"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="5/5"
        android:textSize="18dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.789"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/comentarioampliandorest"
        app:layout_constraintVertical_bias="0.297" />

    <Button
        android:id="@+id/botonreservarrest"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="30dp"
        android:backgroundTint="@color/color2"
        android:text="@string/reservar"
        android:textColor="@color/color6"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/calificaciontitulo"
        app:layout_constraintVertical_bias="0.34" />

</androidx.constraintlayout.widget.ConstraintLayout>